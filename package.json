{
  "name": "messmass",
  "version": "10.3.0",
  "description": "Enterprise-grade event analytics platform with comprehensive partner management, intelligent link tracking, automated event creation (Sports Match Builder), and advanced real-time statistics dashboard for sports organizations, venues, brands, and event managers",
  "keywords": [
    "event-statistics",
    "real-time-collaboration",
    "dashboard",
    "admin-panel",
    "websocket",
    "next.js",
    "mongodb",
    "event-management",
    "audience-tracking",
    "demographics"
  ],
  "author": {
    "name": "Csaba Moldovan",
    "email": "moldovancsaba@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moldovancsaba/messmass.git"
  },
  "homepage": "https://messmass.doneisbetter.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "export": "next export",
    "clean": "rm -rf .next out",
    "style:audit": "node scripts/audit-styles.js",
    "style:check": "node scripts/check-design-violations.js",
    "migrate-slugs": "node migrate-slugs.js",
    "migrate-hashtags": "node scripts/migrate-hashtags.js",
    "migrate-stats-v2.13.0": "node scripts/migrate-stats-v2.13.0.js",
    "analytics:setup-indexes": "tsx scripts/setupAnalyticsIndexes.ts",
    "analytics:aggregate": "tsx scripts/aggregateAnalytics.ts",
    "analytics:backfill": "tsx scripts/backfillAnalytics.ts",
    "migrate:remoteFans": "node scripts/migrate-to-remoteFans.js",
    "migrate:chart-formulas": "node scripts/migrate-chart-formulas.js",
    "migrate:chart-lowercase": "tsx -r dotenv/config scripts/migrateChartFormulasToLowercase.ts dotenv_config_path=.env.local",
    "migrate:absolute-paths": "npx tsx -r dotenv/config scripts/migrateToAbsoluteDbPaths.ts dotenv_config_path=.env.local",
    "data:audit": "tsx scripts/validateDataQuality.ts --audit",
    "data:audit:verbose": "tsx scripts/validateDataQuality.ts --audit --verbose",
    "data:fix": "tsx scripts/validateDataQuality.ts --fix",
    "data:fix:verbose": "tsx scripts/validateDataQuality.ts --fix --verbose",
    "football-data:setup-indexes": "tsx scripts/setupFootballDataIndexes.ts",
    "football-data:sync": "tsx scripts/syncFootballDataFixtures.ts",
    "seed:page-styles": "tsx scripts/seedPageStyles.ts",
    "cleanup:old-styles": "npx tsx scripts/cleanupOldStyleSystem.ts",
    "migrate:style-fields": "tsx -r dotenv/config scripts/migrateStyleIdToEnhanced.ts dotenv_config_path=.env.local",
    "seed:variables": "tsx -r dotenv/config scripts/seedVariablesFromRegistry.ts dotenv_config_path=.env.local",
    "seed:groups": "tsx -r dotenv/config scripts/seedVariableGroups.ts dotenv_config_path=.env.local",
    "seed:visitor-kyc": "node scripts/addVisitorKYCVariables.js",
    "seed:charts": "node scripts/seed-default-charts.js",
    "migrate:show-title": "tsx -r dotenv/config scripts/migrateShowTitleField.ts dotenv_config_path=.env.local",
    "migrate:chart-formatting-cleanup": "tsx -r dotenv/config scripts/migrateChartFormattingCleanup.ts dotenv_config_path=.env.local",
    "migrate:charts-formatting": "tsx -r dotenv/config scripts/migrateChartsToNewFormatting.ts dotenv_config_path=.env.local",
    "seed:value-charts": "tsx -r dotenv/config scripts/seedValueChartTemplates.ts dotenv_config_path=.env.local",
    "seed:formatting-defaults": "tsx -r dotenv/config scripts/seedChartFormattingDefaults.ts dotenv_config_path=.env.local",
    "migrate:existing-charts-formatting": "tsx -r dotenv/config scripts/migrateExistingChartsFormatting.ts dotenv_config_path=.env.local",
    "version:update": "node scripts/update-version.js",
    "version:verify": "node scripts/update-version.js --verify",
    "audit:database": "tsx -r dotenv/config scripts/auditDatabaseCollections.ts dotenv_config_path=.env.local",
    "db:backup": "npx tsx -r dotenv/config scripts/backupDatabase.ts dotenv_config_path=.env.local",
    "db:restore": "npx tsx -r dotenv/config scripts/restoreDatabase.ts dotenv_config_path=.env.local",
    "db:consolidate-bitly": "npx tsx -r dotenv/config scripts/consolidateBitlyJunctions.ts dotenv_config_path=.env.local",
    "db:consolidate-variables": "npx tsx -r dotenv/config scripts/consolidateVariablesConfig.ts dotenv_config_path=.env.local",
    "db:rename-collections": "npx tsx -r dotenv/config scripts/renameCollections.ts dotenv_config_path=.env.local",
    "db:fix-orphaned-styles": "npx tsx -r dotenv/config scripts/fixOrphanedStyleReferences.ts dotenv_config_path=.env.local",
    "db:create-indexes": "npx tsx -r dotenv/config scripts/createMissingIndexes.ts dotenv_config_path=.env.local",
    "db:cleanup-empty": "npx tsx -r dotenv/config scripts/cleanupEmptyCollections.ts dotenv_config_path=.env.local",
    "migrate:emoji-to-icon": "npx tsx -r dotenv/config scripts/migrateEmojiToIcon.ts dotenv_config_path=.env.local",
    "chart:check-icons": "npx tsx -r dotenv/config scripts/checkChartIcons.ts dotenv_config_path=.env.local",
    "chart:fix-icons": "npx tsx -r dotenv/config scripts/fixInvalidIcons.ts dotenv_config_path=.env.local"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "dotenv": "^17.2.1",
    "focus-trap-react": "^11.0.4",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.1",
    "mongodb": "^6.8.0",
    "next": "^15.5.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "typescript": "^5.6.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/html2canvas": "^0.5.35",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.4",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "is-date-object": "^1.1.0",
    "tsx": "^4.20.6"
  },
  "engines": {
    "node": "18.x || 20.x || 22.x",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
